namespace topology

let TopSpace : Type

let empty (X : TopSpace) : Prop

let connected path_connected compact quasi_compact hausdorff
    (X : TopSpace) : Prop

let open_map closed_map proper compact_map quasi_compact_map dense galois_cover
    {X Y : TopSpace} (f : Morphism X Y) : Prop

def homeomorphism {X Y : TopSpace} (f : Morphism X Y) := isomorphism f

end topology

